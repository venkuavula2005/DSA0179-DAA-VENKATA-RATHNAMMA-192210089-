#include<iostream>
using namespace std;
int main(){
    int n,r,rev=0,num;
    cout<<"enter the number";
    cin>>n;
    num=n;
    while(n>0){
        r=n%10;
        rev=rev*10+r;
        n=n/10;
    }
    if(rev==num){
        cout<<"palindrome";
    }else{
        cout<<"not palindrome";
    }
}
